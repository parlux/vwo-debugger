{
  "name": "VWO debugger",
  "version": "0.2",
  "description": "Go debug yourself some VWO campaigns",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "storage"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "vwo-logo.png",
    "48": "vwo-logo.png",
    "128": "vwo-logo.png"
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}